<%= simple_form_for(@capacity) do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
  	<%= f.association :processing_unit, input_html: { class: 'form-control' } %>
  	<%= f.input :effective_date, input_html: { class: 'form-control' } %>
  	<%= f.input :value, input_html: { class: 'form-control' } %>
  </div>

  <div class="actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>
